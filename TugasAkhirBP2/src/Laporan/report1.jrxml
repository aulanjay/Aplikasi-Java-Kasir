     `tbl_barang` tbl_barang INNER JOIN `tbl_detail_jual` tbl_detail_jual ON tbl_barang.`kd_barang` = tbl_detail_jual.`kd_barang`
     INNER JOIN `tbl_penjualan` tbl_penjualan ON tbl_detail_jual.`nonota` = tbl_penjualan.`nota` 
